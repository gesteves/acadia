<meta charset="UTF-8">
<%= inline_stylesheet "base" %>
<meta name="author" content="<%= data.content.meta.author %>">
<meta name="description" content="<%= data.content.meta.description %>">
<meta name="verify-v1" content="<%= data.content.meta.google_validation %>">
<meta name="viewport" content="width=device-width">
<link rel="icon" type="image/png" href="<%= image_url "favicon.png" %>">
<link rel="preload" href="https://use.typekit.net/<%= data.content.meta.typekit_id %>.js" as="script">
<title><%= data.content.meta.title %></title>
<%= javascript_include_tag  "main", defer: 'defer' %>
<%= javascript_include_tag "https://use.typekit.net/#{data.content.meta.typekit_id}.js", async: 'async', onload: 'try{Typekit.load({async:true})}catch(e){}' %>
<% unless data.content.meta.google_analytics_id.nil? %>
<script>(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)})(window,document,'script','//www.google-analytics.com/analytics.js','ga');ga('create','<%= data.content.meta.google_analytics_id %>','auto');ga('send','pageview');</script>
<% end %>
