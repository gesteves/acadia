<meta charset="UTF-8" />
<meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
<meta name="author" content="<%= data.content.meta.author %>" />
<meta name="description" content="<%= data.content.meta.description %>" />
<meta name="verify-v1" content="<%= data.content.meta.google_validation %>" />
<meta name="viewport" content="width=device-width" />
<meta property="fb:admins" content="<%= data.content.meta.fb_admin %>" />
<meta property="og:title" content="<%= data.content.meta.title %>"/>
<meta property="og:url" content="<%= data.content.meta.canonical_url %>"/>
<meta property="og:image" content="<%= image_url "card.png" %>" />
<meta property="og:site_name" content="<%= data.content.meta.title %>"/>
<meta property="og:description" content="<%= data.content.meta.description %>"/>
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:description" content="<%= data.content.meta.description %>" />
<meta name="twitter:title" content="<%= data.content.meta.title %>" />
<meta name="twitter:image:src" content="<%= image_url "card.png" %>" />
<meta name="twitter:url" content="<%= data.content.meta.canonical_url %>" />
<meta name="twitter:site" content="<%= data.content.meta.twitter_username %>" />
<meta name="twitter:creator" content="<%= data.content.meta.twitter_username %>" />
<link href="http://use.typekit.net" rel="preconnect">
<link href="https://use.typekit.net" rel="preconnect">
<link href="https://p.typekit.net" rel="preconnect">
<link rel="icon" type="image/png" href="<%= image_url "favicon.png" %>" />
<title><%= data.content.meta.title %></title>
<link rel="me" type="text/html" href="https://plus.google.com/114617182643828033131"/>
<%= stylesheet_link_tag "main" %>
