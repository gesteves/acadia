<section class="b-card b-photoblog">
<h3>Photoblog</h3>
<% data.photoblog.each do |photo| %>
  <a href="<%= photo.post_url %>">
    <%= photoblog_image_tag(photo) %>
  </a>
  <% unless photo.caption.nil? %>
    <%= photo.caption %>
  <% end %>
  <p class="meta"><%= photo_exif(photo) %></p>
<% end %>
</section>